<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/log"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.log.LogFragment" >

    <ImageButton
        android:id="@+id/btn_product"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/products"
        app:layout_constraintBottom_toTopOf="@+id/textView48"
        app:layout_constraintEnd_toStartOf="@+id/btn_supplier"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/product"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/btn_product"
        app:layout_constraintStart_toStartOf="@+id/btn_product"
        app:layout_constraintTop_toBottomOf="@+id/btn_product" />

    <ImageButton
        android:id="@+id/btn_supplier"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/suppliers"
        app:layout_constraintBottom_toBottomOf="@+id/btn_product"
        app:layout_constraintEnd_toStartOf="@+id/btn_employee"
        app:layout_constraintStart_toEndOf="@+id/btn_product"
        app:layout_constraintTop_toTopOf="@+id/btn_product"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/supplier"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView48"
        app:layout_constraintEnd_toEndOf="@+id/btn_supplier"
        app:layout_constraintStart_toStartOf="@+id/btn_supplier"
        app:layout_constraintTop_toTopOf="@+id/textView48" />

    <ImageButton
        android:id="@+id/btn_employee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/ic_person_black_100dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_supplier"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_supplier"
        app:layout_constraintTop_toTopOf="@+id/btn_supplier" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Pegawai"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView49"
        app:layout_constraintEnd_toEndOf="@+id/btn_employee"
        app:layout_constraintStart_toStartOf="@+id/btn_employee"
        app:layout_constraintTop_toTopOf="@+id/textView49" />
</androidx.constraintlayout.widget.ConstraintLayout>
